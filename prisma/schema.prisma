generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Apartment {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  address     String
  city        String
  state       String
  zipCode     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
